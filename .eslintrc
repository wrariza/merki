{
  "extends": airbnb,
  "parse": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react",
  ],
  "env": {
      "browser": true,
      "node": true,
      "es6": true
  },
  "rules": {
    "semi": ["error", "never"],
    "semi-spacing": ["error", { "before": false, "after": true }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "consistent-return": 0,
    "no-console": 0
  }
}
